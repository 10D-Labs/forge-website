{
  "framework": "nextjs",
  "redirects": [
    {
      "source": "/blog/can-ai-replace-personal-trainer",
      "destination": "/blog/can-ai-replace-personal-trainer-research",
      "permanent": true
    },
    {
      "source": "/topics/strength-training",
      "destination": "/topics/strength-training-basics",
      "permanent": true
    },
    {
      "source": "/topics/fitness-technology",
      "destination": "/topics/ai-fitness-technology",
      "permanent": true
    },
    {
      "source": "/topics/fitness-psychology",
      "destination": "/topics/fitness-motivation",
      "permanent": true
    },
    {
      "source": "/topics/training-fundamentals",
      "destination": "/topics/strength-training-basics",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/markdown/(.*).md",
      "headers": [
        { "key": "Content-Type", "value": "text/markdown; charset=utf-8" },
        { "key": "Cache-Control", "value": "public, max-age=3600" },
        { "key": "X-Robots-Tag", "value": "index, follow" }
      ]
    },
    {
      "source": "/llms.txt",
      "headers": [
        { "key": "Content-Type", "value": "text/plain; charset=utf-8" },
        { "key": "Cache-Control", "value": "public, max-age=3600" },
        { "key": "X-Robots-Tag", "value": "index, follow" }
      ]
    },
    {
      "source": "/llms-full.txt",
      "headers": [
        { "key": "Content-Type", "value": "text/plain; charset=utf-8" },
        { "key": "Cache-Control", "value": "public, max-age=3600" },
        { "key": "X-Robots-Tag", "value": "index, follow" }
      ]
    }
  ]
}
